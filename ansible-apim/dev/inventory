[apim]
apim_1 ansible_host=apim-1 ansible_user=vagrant
apim_2 ansible_host=apim-2 ansible_user=vagrant

# Example
# apim_1 ansible_host=172.28.128.4 ansible_user=vagrant

#[apim-analytics]
#apim-analytics-dashboard_1 ansible_host=[ip_address] ansible_user=[ssh_user]
# Example
# apim-analytics-dashboard_1 ansible_host=172.28.128.4 ansible_user=vagrant

[apim-analytics-worker]
apim-analytics-worker_1 ansible_host=apim-analytics-worker-1 ansible_user=vagrant
apim-analytics-worker_2 ansible_host=apim-analytics-worker-2 ansible_user=vagrant

# Example
# apim-analytics-worker_1 ansible_host=172.28.128.4 ansible_user=vagrant

